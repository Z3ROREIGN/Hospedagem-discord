{
  "functions": {
    "bot": {
      "runtime": "nodejs14.x",
      "handler": "main.handler",
      "environment": {
        "DISCORD_BOT_TOKEN": "${{ secrets.DISCORD_BOT_TOKEN }}"
      }
    }
  },
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ]
}